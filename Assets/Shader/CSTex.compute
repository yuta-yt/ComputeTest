#pragma kernel KernelFunctionA

RWTexture2D<float4> texBuffer;

[numthreads(8,8,1)]
void KernelFunctionA (uint3 dtid : SV_DispatchThreadID)
{
    float width, height;
    texBuffer.GetDimensions(width, height);
    texBuffer[dtid.xy] = float4(dtid.x / width,
                                dtid.x / width,
                                dtid.x / width,
                                1);
}
